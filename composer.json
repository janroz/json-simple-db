{
    "name": "mhujer/json-simple-db",
    "description": "Work with JSON file like with a simple database",
    "version": "1.1.1",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "json",
        "database"
    ],
    "homepage": "https://github.com/mhujer/json-simple-db",
    "authors": [
        {
            "name": "Martin Hujer",
            "email": "mhujer@gmail.com",
            "homepage": "http://www.martinhujer.cz/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "~2.3",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "JsonSimpleDb\\": "src/JsonSimpleDb"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=PSR2 src && phpcs --standard=PSR2 tests"
    }
}
